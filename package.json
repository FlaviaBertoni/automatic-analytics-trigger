{
  "name": "automatic-analytics-trigger",
  "version": "1.0.0",
  "description": "Automatically trigger events and mutations for analytics",
  "main": "main.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest ^.+\\.spec.js",
    "test:integration": "jest -i ^.+\\.test.js",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "npm run lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlaviaBertoni/automatic-analytics-trigger.git"
  },
  "author": "FlaviaBertoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlaviaBertoni/automatic-analytics-trigger/issues"
  },
  "homepage": "https://github.com/FlaviaBertoni/automatic-analytics-trigger#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "html-loader-jest": "^0.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "html"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.html?$": "html-loader-jest"
    }
  },
  "dependencies": {}
}
